{
  "name": "eslint-plugin-authing-nestjs",
  "version": "0.0.5-alpha.1",
  "license": "MIT",
  "description": "ESLint rules for nestjs framework",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "build": "tsc",
    "lint": "eslint --fix --ext .ts src/**.ts"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@types/estree": "^0.0.50",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "lint-staged": "^12.3.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "typescript": "^4.5.5"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --parser=typescript --write"
    ]
  },
  "keywords": [
    "eslint-plugin-authing-nestjs"
  ],
  "author": "https://github.com/authing",
  "repository": {
    "type": "git",
    "url": "https://github.com/Authing/eslint-plugin-nestjs.git"
  },
  "bugs": {
    "url": "https://github.com/Authing/eslint-plugin-nestjs/issues"
  },
  "homepage": "https://github.com/Authing/eslint-plugin-nestjs#readme"
}
